<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/mine_list">
    <LinearLayout
        android:id="@+id/want_movie"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:background="#ffffff">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:paddingLeft="10dp"
            android:text="想看的电影"
            android:textColor="#000000"/>


        <ImageView
            android:scaleType="fitEnd"
            android:padding="30dp"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:src="@drawable/mine_arrow_gray"/>

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d8d8d8"/>
    <LinearLayout
        android:id="@+id/watched_movie"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:background="#ffffff">

        <TextView
            android:paddingLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="看过的电影"
            android:textColor="#000000"/>

        <ImageView
            android:scaleType="fitEnd"
            android:padding="30dp"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:src="@drawable/mine_arrow_gray" />

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d8d8d8"/>
    <LinearLayout
        android:id="@+id/mine_comm"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:background="#ffffff">

        <TextView
            android:paddingLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="我的西虹市社区"
            android:textColor="#000000"/>

        <ImageView
            android:scaleType="fitEnd"
            android:padding="30dp"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:src="@drawable/mine_arrow_gray" />

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d8d8d8"/>
    <LinearLayout
        android:id="@+id/mine_little"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:background="#ffffff">

        <TextView
            android:paddingLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="我的小聚场"
            android:textColor="#000000"/>

        <ImageView
            android:scaleType="fitEnd"
            android:padding="30dp"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:src="@drawable/mine_arrow_gray" />

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d8d8d8"/>
    <LinearLayout
        android:id="@+id/mine_discussion"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:background="#ffffff">

        <TextView
            android:paddingLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="发起的讨论"
            android:textColor="#000000"/>

        <ImageView
            android:scaleType="fitEnd"
            android:padding="30dp"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:src="@drawable/mine_arrow_gray" />

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d8d8d8"/>
    <LinearLayout
        android:id="@+id/mine_redpack"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:background="#ffffff">

        <TextView
            android:paddingLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="发红包赚赏金"
            android:textColor="#000000"/>

        <ImageView
            android:scaleType="fitEnd"
            android:padding="30dp"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:src="@drawable/mine_arrow_gray" />

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d8d8d8"/>
    <LinearLayout
        android:id="@+id/mine_bankcard"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:background="#ffffff">

        <TextView
            android:paddingLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="银行卡特惠"
            android:textColor="#000000"/>

        <ImageView
            android:scaleType="fitEnd"
            android:padding="30dp"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:src="@drawable/mine_arrow_gray" />

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d8d8d8"/>
    <LinearLayout
        android:id="@+id/mine_mall"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:background="#ffffff">

        <TextView
            android:paddingLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="阿里鱼商城"
            android:textColor="#000000"/>

        <ImageView
            android:scaleType="fitEnd"
            android:padding="30dp"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:src="@drawable/mine_arrow_gray" />

    </LinearLayout>
    <View
    android:layout_width="match_parent"
    android:layout_height="1dp"
    android:background="#d8d8d8"/>
    <LinearLayout
        android:id="@+id/mine_entertainment"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:background="#ffffff">

        <TextView
            android:paddingLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="娱乐宝"
            android:textColor="#000000"/>

        <ImageView
            android:scaleType="fitEnd"
            android:padding="30dp"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:src="@drawable/mine_arrow_gray" />

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d8d8d8"/>
    <LinearLayout
        android:id="@+id/mine_help"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:background="#ffffff">

        <TextView
            android:paddingLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="帮助与反馈"
            android:textColor="#000000"/>

        <ImageView
            android:scaleType="fitEnd"
            android:padding="30dp"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:src="@drawable/mine_arrow_gray" />

    </LinearLayout>


</LinearLayout>
